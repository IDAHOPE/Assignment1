#include <stdio.h>
int is_palindrom(int n)
{
  int r= 0, t = n;
  while (t) 
  {
    r= 10*r + (t % 10);
    t /= 10;
  }
  return r == n;
}
int main()
{
  int i, j, max = 0;
  for (i = 100; i <= 999; i++) 
  {
    for (j = 100; j <= 999; j++) 
    {
      int p = i*j;
      if (is_palindrom(p) && p > max) 
      {
        max = p;
      }
    }
  }
  printf("%d\n", max);
  return 0;
}
